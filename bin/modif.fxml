<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane prefHeight="300.0" prefWidth="439.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vue.CtrlModif">
    <children>
        <Label layoutX="20" layoutY="20" style="-fx-font-size: 16px;" text="Modification Inscrit" />

        <Label layoutX="20" layoutY="60" text="ID :" />
        <TextField fx:id="idField" editable="false" layoutX="100" layoutY="55" prefHeight="29.0" prefWidth="176.0" />

        <Label layoutX="20" layoutY="100" text="Nom :" />
        <TextField fx:id="nomField" layoutX="100" layoutY="95" prefWidth="200" />

        <Label layoutX="20" layoutY="140" text="Prénom :" />
        <TextField fx:id="prenomField" layoutX="100" layoutY="135" prefWidth="200" />

        <Label layoutX="20" layoutY="180" prefHeight="19.0" prefWidth="107.0" text="Montant payé :" />
      <TextField fx:id="montantField" layoutX="130.0" layoutY="175.0" prefHeight="29.0" prefWidth="189.0" />

        <Label layoutX="20" layoutY="220" text="Date du paiement :" />
        <DatePicker fx:id="datePaiementPicker" layoutX="148.0" layoutY="215.0" />

        <Button fx:id="bnSuppMembre" layoutX="280.0" layoutY="55" prefHeight="29.0" prefWidth="146.0" style="-fx-text-fill: red;" text="Supprimer membre" />

        <Button fx:id="bnEnregistrer" layoutX="266.0" layoutY="260.0" text="Enregistrer" />
        <Button fx:id="bnAnnuler" layoutX="359.0" layoutY="260.0" onAction="#clicAnnuler" text="Annuler" />
    </children>
</AnchorPane>
